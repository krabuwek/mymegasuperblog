<div ng-app="dsd" ng-controller="MainController">
	<p>{{ loaded }}</p>
	<div ng-repeat="post in data">
    <div class="title">
      <h1>{{ post.attributes.title }}</h1>
    </div>
    <div class="body">
      {{ post.attributes.body | limitTo:300 }}...
    </div>
    <div class="time">
      {{ post.attributes.created-at }}
    </div>
    <div class="user-email">
      email:
    </div>
	</div>
</div>
